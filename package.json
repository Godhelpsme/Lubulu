{
  "name": "lubulu",
  "version": "2.1.0",
  "description": "Lubulu - 健康决策轮盘 PWA 应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "pages:deploy": "npm run build && wrangler pages deploy dist",
    "api:dev": "cd workers/api && npm run dev",
    "api:deploy": "cd workers/api && npm run deploy"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-legacy": "^5.0.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "wrangler": "^3.0.0"
  },
  "keywords": [
    "pwa",
    "health",
    "decision",
    "roulette",
    "cloudflare"
  ],
  "author": "Lubulu Team",
  "license": "MIT"
}
